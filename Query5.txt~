SELECT R.REGION_Desc, '0-25' AS AgeGroup, SUM(P.POPESTIMATE2011) FROM POP_EST_STATE_AGE_SEX_RACE_ORIGIN P, REGION R WHERE P.REGION = R.REGION_Cd AND P.AGE < 26 AND P.SEX = 0 AND P.ORIGIN = 0 GROUP BY R.REGION_Cd UNION SELECT R.REGION_Desc, '26-50' AS AgeGroup, SUM(P.POPESTIMATE2011) FROM POP_EST_STATE_AGE_SEX_RACE_ORIGIN P, REGION R WHERE P.REGION = R.REGION_Cd AND P.SEX = 0 AND P.ORIGIN = 0 AND P.AGE > 26 AND P.AGE < 50 GROUP BY R.REGION_Cd;
